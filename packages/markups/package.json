{
    "name": "@embeddedchat/markups",
    "version": "0.0.0",
    "description": "A React component library for handling markup interactions in EmbeddedChat",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/RocketChat/EmbeddedChat"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "rollup -c --context=window --environment NODE_ENV:production",
        "watch": "rollup -c --watch --context=window",
        "test:lint": "eslint src/**/*.js",
        "format": "prettier --write 'src/' ",
        "format:check": "prettier --check 'src/' ",
        "lint": "eslint 'src/**/*.js'",
        "lint:fix": "eslint 'src/**/*.js' --fix"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
