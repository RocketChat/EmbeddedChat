{
    "parser": "@babel/eslint-parser",
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error",
      "no-console": "off",
      "no-unused-vars": "warn",
      "no-underscore-dangle": "off",
      "no-param-reassign": "off",
      "consistent-return": "off",
      "import/no-extraneous-dependencies": "off",
      "no-nested-ternary": "off",
      "no-undef": "error",
      "no-restricted-exports": "off"
    }
  }
  